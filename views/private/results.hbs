<h1>results</h1>
{{#each results}}
<div>
    <h3>Outfit</h3>
    <img class="outfit-image" src="{{top.imageUrl}}" alt="Result Img">
    <img class="outfit-image" src="{{bottom.imageUrl}}" alt="Result Img">
    <img class="outfit-image" src="{{shoes.imageUrl}}" alt="Result Img">
</div>
<form action="/outfit/{{id}}/add" method="post">
    <button type="submit">add to collection</button>
</form>
{{/each}}